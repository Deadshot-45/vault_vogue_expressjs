{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      
        "headers": [
          {
            "source": "/(.*)\\.(?:png|jpg|jpeg|gif|svg|ico)$",
            "headers": [
              {
                "key": "Cross-Origin-Resource-Policy",
                "value": "cross-origin"
              }
            ]
          }
        ]
      }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
